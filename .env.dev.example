# Development Environment Variables
# Copy this file to .env.dev and fill in your values

# Tycho API Configuration
TYCHO_API_KEY=sampletoken

# Tycho Chain URLs
TYCHO_ETHEREUM_URL=tycho-beta.propellerheads.xyz
TYCHO_BASE_URL=tycho-base-beta.propellerheads.xyz
TYCHO_UNICHAIN_URL=tycho-unichain-beta.propellerheads.xyz

# Logging Configuration
RUST_LOG=debug

# TVL Threshold (in eth)
TVL_THRESHOLD=100

# Port Configuration (REQUIRED - no defaults)
# For multiple worktrees, use different port ranges (e.g., 5001-5003, 6001-6003)
TYCHO_API_ETHEREUM_PORT=4001
TYCHO_API_BASE_PORT=4002
TYCHO_API_UNICHAIN_PORT=4003
FRONTEND_DEV_PORT=5173

# Frontend WebSocket URLs (required for development)
# Update these to match your port configuration above
VITE_WEBSOCKET_URL_ETHEREUM=ws://localhost:4001/ws
VITE_WEBSOCKET_URL_BASE=ws://localhost:4002/ws
VITE_WEBSOCKET_URL_UNICHAIN=ws://localhost:4003/ws

# Frontend API URLs (for REST endpoints like simulation)
# Update these to match your port configuration above
VITE_API_ETHEREUM_URL=http://localhost:4001
VITE_API_BASE_URL=http://localhost:4002
VITE_API_UNICHAIN_URL=http://localhost:4003