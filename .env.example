# Development Environment Variables
# Copy this file to .env and fill in your values

# Tycho API Configuration
TYCHO_API_KEY=sampletoken

# Tycho Chain URLs
TYCHO_ETHEREUM_URL=tycho-beta.propellerheads.xyz
TYCHO_BASE_URL=tycho-base-beta.propellerheads.xyz
TYCHO_UNICHAIN_URL=tycho-unichain-beta.propellerheads.xyz

# Logging Configuration
RUST_LOG=debug

# TVL Threshold (in eth)
TVL_THRESHOLD=100

# TODO: change me
# RPC_URL for VM based protocols (e.g. for contract bytecode fetching)
RPC_URL=URL

# Frontend WebSocket URLs (required for development)
VITE_WEBSOCKET_URL_ETHEREUM=ws://localhost:3001/ws
VITE_WEBSOCKET_URL_BASE=ws://localhost:3002/ws
VITE_WEBSOCKET_URL_UNICHAIN=ws://localhost:3003/ws

# Frontend API URLs (for REST endpoints like simulation)
# Production: Use relative paths (nginx will proxy)
# Development: Use absolute URLs with port numbers
VITE_API_ETHEREUM_URL=/ethereum
VITE_API_BASE_URL=/base
VITE_API_UNICHAIN_URL=/unichain